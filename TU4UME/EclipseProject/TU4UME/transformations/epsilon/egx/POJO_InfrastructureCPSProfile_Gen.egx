//generate Java POJO classes for each stereotype
rule Stereotype2POJO	
	transform stereo : Stereotype {
	
		template : "../egl/POJO_InfrastructureCPSProfile_Stereotype_GenTemplate.egl"
		target : "../../../../TU4UME/src/at/ac/tuwien/dsg/uml/profiles/infrastructurecpsprofile/stereotypes/pojos/" + stereo.name + ".java"
	}

rule Enum2POJO	
	transform enum : Enumeration {
	
		template : "../egl/POJO_InfrastructureCPSProfile_Enumeration_GenTemplate.egl"
		target : "../../../../TU4UME/src/at/ac/tuwien/dsg/uml/profiles/infrastructurecpsprofile/enumerations/pojos/" + enum.name + ".java"
	}	

