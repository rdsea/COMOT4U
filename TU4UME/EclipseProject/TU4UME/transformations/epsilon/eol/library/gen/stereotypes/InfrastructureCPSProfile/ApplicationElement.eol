/*
Check if the element is annotated with ApplicationElement.
*/ 
operation Element isApplicationElement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isApplicationElement() : Boolean in ApplicationElement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "ApplicationElement");
}



/*
Returns a Sequence of Property defined in ApplicationElement.
*/ 
operation ApplicationElement getAllAttributesOfApplicationElement() : Sequence(Property){
	var stereotypeAttr = ApplicationElement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of applicationInterfaces of stereotype ApplicationElement.
*/ 
operation Element getApplicationInterfacesOfApplicationElementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("ApplicationElement"), "applicationInterfaces");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of applicationInterfaces of stereotype ApplicationElement is empty.
*/ 
operation Element isApplicationInterfacesOfApplicationElementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("ApplicationElement"), "applicationInterfaces");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}


operation Stereotype getApplicationInterfacesProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var applicationInterfacesProperty = allAttributes.selectOne(s|s.name = "applicationInterfaces");
	return applicationInterfacesProperty;
}

