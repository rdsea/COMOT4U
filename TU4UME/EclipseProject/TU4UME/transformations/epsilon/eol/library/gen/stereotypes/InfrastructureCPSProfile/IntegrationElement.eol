/*
Check if the element is annotated with IntegrationElement.
*/ 
operation Element isIntegrationElement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isIntegrationElement() : Boolean in IntegrationElement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "IntegrationElement");
}



/*
Returns a Sequence of Property defined in IntegrationElement.
*/ 
operation IntegrationElement getAllAttributesOfIntegrationElement() : Sequence(Property){
	var stereotypeAttr = IntegrationElement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of integrationInterfaces of stereotype IntegrationElement.
*/ 
operation Element getIntegrationInterfacesOfIntegrationElementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IntegrationElement"), "integrationInterfaces");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of integrationInterfaces of stereotype IntegrationElement is empty.
*/ 
operation Element isIntegrationInterfacesOfIntegrationElementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IntegrationElement"), "integrationInterfaces");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}


operation Stereotype getIntegrationInterfacesProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var integrationInterfacesProperty = allAttributes.selectOne(s|s.name = "integrationInterfaces");
	return integrationInterfacesProperty;
}

