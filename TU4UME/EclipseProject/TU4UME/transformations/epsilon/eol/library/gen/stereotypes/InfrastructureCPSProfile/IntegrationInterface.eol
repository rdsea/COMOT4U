/*
Return the BaseElement from the applied Stereotype IntegrationInterface using UMLUtil native object
*/ 
operation IntegrationInterface getBaseElementOfIntegrationInterface() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.tu4ume.epsilon.externaltools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

