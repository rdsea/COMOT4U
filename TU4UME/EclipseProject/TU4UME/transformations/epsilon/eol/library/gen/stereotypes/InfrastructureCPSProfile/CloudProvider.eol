/*
Check if the element is annotated with CloudProvider.
*/ 
operation Element isCloudProvider() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isCloudProvider() : Boolean in CloudProvider.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "CloudProvider");
}



/*
Returns a Sequence of Property defined in CloudProvider.
*/ 
operation CloudProvider getAllAttributesOfCloudProvider() : Sequence(Property){
	var stereotypeAttr = CloudProvider.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of id of stereotype CloudProvider.
*/ 
operation Element getIdOfCloudProviderAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("CloudProvider"), "id");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of id of stereotype CloudProvider is empty.
*/ 
operation Element isIdOfCloudProviderEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("CloudProvider"), "id");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}


operation Stereotype getIdProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var idProperty = allAttributes.selectOne(s|s.name = "id");
	return idProperty;
}

