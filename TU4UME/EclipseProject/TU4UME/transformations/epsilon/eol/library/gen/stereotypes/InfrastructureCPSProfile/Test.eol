/*
Check if the element is annotated with Test.
*/ 
operation Element isTest() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTest() : Boolean in Test.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "Test");
}



/*
Returns a Sequence of Property defined in Test.
*/ 
operation Test getAllAttributesOfTest() : Sequence(Property){
	var stereotypeAttr = Test.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}



