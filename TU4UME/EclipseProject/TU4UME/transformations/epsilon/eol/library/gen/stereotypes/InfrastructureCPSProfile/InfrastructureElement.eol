/*
Check if the element is annotated with InfrastructureElement.
*/ 
operation Element isInfrastructureElement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isInfrastructureElement() : Boolean in InfrastructureElement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "InfrastructureElement");
}



/*
Returns a Sequence of Property defined in InfrastructureElement.
*/ 
operation InfrastructureElement getAllAttributesOfInfrastructureElement() : Sequence(Property){
	var stereotypeAttr = InfrastructureElement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of infrastructureInterface of stereotype InfrastructureElement.
*/ 
operation Element getInfrastructureInterfaceOfInfrastructureElementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("InfrastructureElement"), "infrastructureInterface");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of infrastructureInterface of stereotype InfrastructureElement is empty.
*/ 
operation Element isInfrastructureInterfaceOfInfrastructureElementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("InfrastructureElement"), "infrastructureInterface");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}


operation Stereotype getInfrastructureInterfaceProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var infrastructureInterfaceProperty = allAttributes.selectOne(s|s.name = "infrastructureInterface");
	return infrastructureInterfaceProperty;
}

