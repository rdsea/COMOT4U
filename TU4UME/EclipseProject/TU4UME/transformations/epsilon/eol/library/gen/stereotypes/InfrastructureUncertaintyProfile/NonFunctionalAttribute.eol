/*
Check if the element is annotated with NonFunctionalAttribute.
*/ 
operation Element isNonFunctionalAttribute() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isNonFunctionalAttribute() : Boolean in NonFunctionalAttribute.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "NonFunctionalAttribute");
}



/*
Returns a Sequence of Property defined in NonFunctionalAttribute.
*/ 
operation NonFunctionalAttribute getAllAttributesOfNonFunctionalAttribute() : Sequence(Property){
	var stereotypeAttr = NonFunctionalAttribute.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of dimensionality of stereotype NonFunctionalAttribute.
*/ 
operation Element getDimensionalityOfNonFunctionalAttributeAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("NonFunctionalAttribute"), "dimensionality");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

