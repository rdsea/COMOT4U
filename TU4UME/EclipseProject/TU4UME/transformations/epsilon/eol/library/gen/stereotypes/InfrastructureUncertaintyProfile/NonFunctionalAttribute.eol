/*
Check if a Sequence of dimensionality of stereotype NonFunctionalAttribute is empty.
*/ 
operation Element isDimensionalityOfNonFunctionalAttributeEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("NonFunctionalAttribute"), "dimensionality");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
