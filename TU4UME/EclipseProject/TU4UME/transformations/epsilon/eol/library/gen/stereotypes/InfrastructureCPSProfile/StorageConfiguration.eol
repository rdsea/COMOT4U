/*
Check if the element is annotated with StorageConfiguration.
*/ 
operation Element isStorageConfiguration() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isStorageConfiguration() : Boolean in StorageConfiguration.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "StorageConfiguration");
}



/*
Returns a Sequence of Property defined in StorageConfiguration.
*/ 
operation StorageConfiguration getAllAttributesOfStorageConfiguration() : Sequence(Property){
	var stereotypeAttr = StorageConfiguration.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype StorageConfiguration.
*/ 
operation Element getDescriptionOfStorageConfigurationAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("StorageConfiguration"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

/*
Returns a Sequence of id of stereotype StorageConfiguration.
*/ 
operation Element getIdOfStorageConfigurationAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("StorageConfiguration"), "id");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype StorageConfiguration is empty.
*/ 
operation Element isDescriptionOfStorageConfigurationEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("StorageConfiguration"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
/*
Check if a Sequence of id of stereotype StorageConfiguration is empty.
*/ 
operation Element isIdOfStorageConfigurationEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("StorageConfiguration"), "id");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}


operation Stereotype getDescriptionProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var descriptionProperty = allAttributes.selectOne(s|s.name = "description");
	return descriptionProperty;
}


operation Stereotype getIdProperty() : Property {
	var allAttributes = self.getAllAttributes();
	var idProperty = allAttributes.selectOne(s|s.name = "id");
	return idProperty;
}

