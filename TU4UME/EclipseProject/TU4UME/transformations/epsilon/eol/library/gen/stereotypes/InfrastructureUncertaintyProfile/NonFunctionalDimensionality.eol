/*
Check if the element is annotated with NonFunctionalDimensionality.
*/ 
operation Element isNonFunctionalDimensionality() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isNonFunctionalDimensionality() : Boolean in NonFunctionalDimensionality.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "NonFunctionalDimensionality");
}



/*
Returns a Sequence of Property defined in NonFunctionalDimensionality.
*/ 
operation NonFunctionalDimensionality getAllAttributesOfNonFunctionalDimensionality() : Sequence(Property){
	var stereotypeAttr = NonFunctionalDimensionality.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype NonFunctionalDimensionality.
*/ 
operation Element getDescriptionOfNonFunctionalDimensionalityAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("NonFunctionalDimensionality"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

