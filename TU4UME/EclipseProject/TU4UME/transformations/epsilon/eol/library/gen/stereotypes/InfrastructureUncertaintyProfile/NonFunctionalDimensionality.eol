/*
Check if a Sequence of description of stereotype NonFunctionalDimensionality is empty.
*/ 
operation Element isDescriptionOfNonFunctionalDimensionalityEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("NonFunctionalDimensionality"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
