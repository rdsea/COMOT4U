/*
Return the BaseElement from the applied Stereotype EffectMeasurement using UMLUtil native object
*/ 
operation EffectMeasurement getBaseElementOfEffectMeasurement() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype EffectMeasurement using UMLUtil native object
*/ 
//operation EffectMeasurement toJsonEffectMeasurement() : String {
operation toJsonEffectMeasurement() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with EffectMeasurement.
*/ 
operation Element isEffectMeasurement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEffectMeasurement() : Boolean in EffectMeasurement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "EffectMeasurement");
}



/*
Returns a Sequence of Property defined in EffectMeasurement.
*/ 
operation EffectMeasurement getAllAttributesOfEffectMeasurement() : Sequence(Property){
	var stereotypeAttr = EffectMeasurement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype EffectMeasurement.
*/ 
operation Element getDescriptionOfEffectMeasurementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("EffectMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype EffectMeasurement is empty.
*/ 
operation Element isDescriptionOfEffectMeasurementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("EffectMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
