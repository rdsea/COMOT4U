/*
Return the BaseElement from the applied Stereotype PeriodicTrigger using UMLUtil native object
*/ 
operation PeriodicTrigger getBaseElementOfPeriodicTrigger() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype PeriodicTrigger using UMLUtil native object
*/ 
//operation PeriodicTrigger toJsonPeriodicTrigger() : String {
operation toJsonPeriodicTrigger() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with PeriodicTrigger.
*/ 
operation Element isPeriodicTrigger() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isPeriodicTrigger() : Boolean in PeriodicTrigger.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "PeriodicTrigger");
}



/*
Returns a Sequence of Property defined in PeriodicTrigger.
*/ 
operation PeriodicTrigger getAllAttributesOfPeriodicTrigger() : Sequence(Property){
	var stereotypeAttr = PeriodicTrigger.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of period of stereotype PeriodicTrigger.
*/ 
operation Element getPeriodOfPeriodicTriggerAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("PeriodicTrigger"), "period");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of period of stereotype PeriodicTrigger is empty.
*/ 
operation Element isPeriodOfPeriodicTriggerEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("PeriodicTrigger"), "period");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
