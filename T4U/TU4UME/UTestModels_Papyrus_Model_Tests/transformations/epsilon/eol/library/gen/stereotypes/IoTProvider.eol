/*
Return the BaseElement from the applied Stereotype IoTProvider using UMLUtil native object
*/ 
operation IoTProvider getBaseElementOfIoTProvider() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype IoTProvider using UMLUtil native object
*/ 
//operation IoTProvider toJsonIoTProvider() : String {
operation toJsonIoTProvider() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with IoTProvider.
*/ 
operation Element isIoTProvider() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isIoTProvider() : Boolean in IoTProvider.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "IoTProvider");
}



/*
Returns a Sequence of Property defined in IoTProvider.
*/ 
operation IoTProvider getAllAttributesOfIoTProvider() : Sequence(Property){
	var stereotypeAttr = IoTProvider.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of id of stereotype IoTProvider.
*/ 
operation Element getIdOfIoTProviderAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IoTProvider"), "id");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of id of stereotype IoTProvider is empty.
*/ 
operation Element isIdOfIoTProviderEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IoTProvider"), "id");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
