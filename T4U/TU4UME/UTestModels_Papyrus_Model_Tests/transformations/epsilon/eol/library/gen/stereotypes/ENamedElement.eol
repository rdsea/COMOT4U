/*
Return the BaseElement from the applied Stereotype ENamedElement using UMLUtil native object
*/ 
operation ENamedElement getBaseElementOfENamedElement() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype ENamedElement using UMLUtil native object
*/ 
//operation ENamedElement toJsonENamedElement() : String {
operation toJsonENamedElement() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with ENamedElement.
*/ 
operation Element isENamedElement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isENamedElement() : Boolean in ENamedElement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "ENamedElement");
}



/*
Returns a Sequence of Property defined in ENamedElement.
*/ 
operation ENamedElement getAllAttributesOfENamedElement() : Sequence(Property){
	var stereotypeAttr = ENamedElement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of xmlName of stereotype ENamedElement.
*/ 
operation Element getXmlNameOfENamedElementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("ENamedElement"), "xmlName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

