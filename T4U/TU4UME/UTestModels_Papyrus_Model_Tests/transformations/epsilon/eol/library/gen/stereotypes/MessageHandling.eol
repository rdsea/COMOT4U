/*
Return the BaseElement from the applied Stereotype MessageHandling using UMLUtil native object
*/ 
operation MessageHandling getBaseElementOfMessageHandling() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype MessageHandling using UMLUtil native object
*/ 
//operation MessageHandling toJsonMessageHandling() : String {
operation toJsonMessageHandling() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with MessageHandling.
*/ 
operation Element isMessageHandling() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMessageHandling() : Boolean in MessageHandling.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "MessageHandling");
}



/*
Returns a Sequence of Property defined in MessageHandling.
*/ 
operation MessageHandling getAllAttributesOfMessageHandling() : Sequence(Property){
	var stereotypeAttr = MessageHandling.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of messageMode of stereotype MessageHandling.
*/ 
operation Element getMessageModeOfMessageHandlingAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MessageHandling"), "messageMode");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

/*
Returns a Sequence of customTemplate of stereotype MessageHandling.
*/ 
operation Element getCustomTemplateOfMessageHandlingAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MessageHandling"), "customTemplate");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of messageMode of stereotype MessageHandling is empty.
*/ 
operation Element isMessageModeOfMessageHandlingEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MessageHandling"), "messageMode");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
/*
Check if a Sequence of customTemplate of stereotype MessageHandling is empty.
*/ 
operation Element isCustomTemplateOfMessageHandlingEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MessageHandling"), "customTemplate");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
