/*
Return the BaseElement from the applied Stereotype RiskMeasurement using UMLUtil native object
*/ 
operation RiskMeasurement getBaseElementOfRiskMeasurement() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype RiskMeasurement using UMLUtil native object
*/ 
//operation RiskMeasurement toJsonRiskMeasurement() : String {
operation toJsonRiskMeasurement() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with RiskMeasurement.
*/ 
operation Element isRiskMeasurement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isRiskMeasurement() : Boolean in RiskMeasurement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "RiskMeasurement");
}



/*
Returns a Sequence of Property defined in RiskMeasurement.
*/ 
operation RiskMeasurement getAllAttributesOfRiskMeasurement() : Sequence(Property){
	var stereotypeAttr = RiskMeasurement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype RiskMeasurement.
*/ 
operation Element getDescriptionOfRiskMeasurementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("RiskMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype RiskMeasurement is empty.
*/ 
operation Element isDescriptionOfRiskMeasurementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("RiskMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
