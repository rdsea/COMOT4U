/*
Check if the element is annotated with IndeterminacyDegreeMeasure.
*/ 
operation Element isIndeterminacyDegreeMeasure() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isIndeterminacyDegreeMeasure() : Boolean in IndeterminacyDegreeMeasure.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "IndeterminacyDegreeMeasure");
}



/*
Returns a Sequence of Property defined in IndeterminacyDegreeMeasure.
*/ 
operation IndeterminacyDegreeMeasure getAllAttributesOfIndeterminacyDegreeMeasure() : Sequence(Property){
	var stereotypeAttr = IndeterminacyDegreeMeasure.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype IndeterminacyDegreeMeasure.
*/ 
operation Element getDescriptionOfIndeterminacyDegreeMeasureAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IndeterminacyDegreeMeasure"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype IndeterminacyDegreeMeasure is empty.
*/ 
operation Element isDescriptionOfIndeterminacyDegreeMeasureEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IndeterminacyDegreeMeasure"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
