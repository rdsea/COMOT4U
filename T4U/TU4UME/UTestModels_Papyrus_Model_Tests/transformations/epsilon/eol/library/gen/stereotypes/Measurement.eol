/*
Return the BaseElement from the applied Stereotype Measurement using UMLUtil native object
*/ 
operation Measurement getBaseElementOfMeasurement() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype Measurement using UMLUtil native object
*/ 
//operation Measurement toJsonMeasurement() : String {
operation toJsonMeasurement() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with Measurement.
*/ 
operation Element isMeasurement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMeasurement() : Boolean in Measurement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "Measurement");
}



/*
Returns a Sequence of Property defined in Measurement.
*/ 
operation Measurement getAllAttributesOfMeasurement() : Sequence(Property){
	var stereotypeAttr = Measurement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of evidence of stereotype Measurement.
*/ 
operation Element getEvidenceOfMeasurementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("Measurement"), "evidence");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

