/*
Return the BaseElement from the applied Stereotype UncertaintyMeasurement using UMLUtil native object
*/ 
operation UncertaintyMeasurement getBaseElementOfUncertaintyMeasurement() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype UncertaintyMeasurement using UMLUtil native object
*/ 
//operation UncertaintyMeasurement toJsonUncertaintyMeasurement() : String {
operation toJsonUncertaintyMeasurement() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with UncertaintyMeasurement.
*/ 
operation Element isUncertaintyMeasurement() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isUncertaintyMeasurement() : Boolean in UncertaintyMeasurement.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "UncertaintyMeasurement");
}



/*
Returns a Sequence of Property defined in UncertaintyMeasurement.
*/ 
operation UncertaintyMeasurement getAllAttributesOfUncertaintyMeasurement() : Sequence(Property){
	var stereotypeAttr = UncertaintyMeasurement.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype UncertaintyMeasurement.
*/ 
operation Element getDescriptionOfUncertaintyMeasurementAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("UncertaintyMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype UncertaintyMeasurement is empty.
*/ 
operation Element isDescriptionOfUncertaintyMeasurementEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("UncertaintyMeasurement"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
