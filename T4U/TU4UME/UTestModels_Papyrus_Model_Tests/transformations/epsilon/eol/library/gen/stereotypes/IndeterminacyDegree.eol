operation toJsonIndeterminacyDegree() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with IndeterminacyDegree.
*/ 
operation Element isIndeterminacyDegree() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isIndeterminacyDegree() : Boolean in IndeterminacyDegree.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "IndeterminacyDegree");
}



/*
Returns a Sequence of Property defined in IndeterminacyDegree.
*/ 
operation IndeterminacyDegree getAllAttributesOfIndeterminacyDegree() : Sequence(Property){
	var stereotypeAttr = IndeterminacyDegree.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype IndeterminacyDegree.
*/ 
operation Element getDescriptionOfIndeterminacyDegreeAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IndeterminacyDegree"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype IndeterminacyDegree is empty.
*/ 
operation Element isDescriptionOfIndeterminacyDegreeEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IndeterminacyDegree"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
