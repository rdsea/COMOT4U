/*
Return the BaseElement from the applied Stereotype EPackage using UMLUtil native object
*/ 
operation EPackage getBaseElementOfEPackage() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype EPackage using UMLUtil native object
*/ 
//operation EPackage toJsonEPackage() : String {
operation toJsonEPackage() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with EPackage.
*/ 
operation Element isEPackage() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEPackage() : Boolean in EPackage.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "EPackage");
}



/*
Returns a Sequence of Property defined in EPackage.
*/ 
operation EPackage getAllAttributesOfEPackage() : Sequence(Property){
	var stereotypeAttr = EPackage.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of packageName of stereotype EPackage.
*/ 
operation Element getPackageNameOfEPackageAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("EPackage"), "packageName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}

