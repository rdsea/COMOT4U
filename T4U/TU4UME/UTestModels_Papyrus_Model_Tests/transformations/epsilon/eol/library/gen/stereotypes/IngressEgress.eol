/*
Check if a Sequence of type of stereotype IngressEgress is empty.
*/ 
operation Element isTypeOfIngressEgressEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("IngressEgress"), "type");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
