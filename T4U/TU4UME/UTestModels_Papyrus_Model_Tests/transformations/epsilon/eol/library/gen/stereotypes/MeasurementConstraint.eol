/*
Return the BaseElement from the applied Stereotype MeasurementConstraint using UMLUtil native object
*/ 
operation MeasurementConstraint getBaseElementOfMeasurementConstraint() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype MeasurementConstraint using UMLUtil native object
*/ 
//operation MeasurementConstraint toJsonMeasurementConstraint() : String {
operation toJsonMeasurementConstraint() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with MeasurementConstraint.
*/ 
operation Element isMeasurementConstraint() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMeasurementConstraint() : Boolean in MeasurementConstraint.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "MeasurementConstraint");
}



/*
Returns a Sequence of Property defined in MeasurementConstraint.
*/ 
operation MeasurementConstraint getAllAttributesOfMeasurementConstraint() : Sequence(Property){
	var stereotypeAttr = MeasurementConstraint.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype MeasurementConstraint.
*/ 
operation Element getDescriptionOfMeasurementConstraintAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MeasurementConstraint"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype MeasurementConstraint is empty.
*/ 
operation Element isDescriptionOfMeasurementConstraintEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MeasurementConstraint"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
