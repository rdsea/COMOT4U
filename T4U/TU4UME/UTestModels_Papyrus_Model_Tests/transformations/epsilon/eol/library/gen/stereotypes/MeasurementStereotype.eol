/*
Return the BaseElement from the applied Stereotype MeasurementStereotype using UMLUtil native object
*/ 
operation MeasurementStereotype getBaseElementOfMeasurementStereotype() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype MeasurementStereotype using UMLUtil native object
*/ 
//operation MeasurementStereotype toJsonMeasurementStereotype() : String {
operation toJsonMeasurementStereotype() : String {

	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	
	var baseElement_string = baseElement.asString();
	var gson = plugin.getGson();
	gson = new GsonBuilder().setPrettyPrinting().create();
	var jsonOutput = gson.toJson(baseElement_string);	
	
	return jsonOutput;
}


/*
Check if the element is annotated with MeasurementStereotype.
*/ 
operation Element isMeasurementStereotype() : Boolean {
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMeasurementStereotype() : Boolean in MeasurementStereotype.eol");
	}
	return self.getAppliedStereotypes()->exists(s | s.name = "MeasurementStereotype");
}



/*
Returns a Sequence of Property defined in MeasurementStereotype.
*/ 
operation MeasurementStereotype getAllAttributesOfMeasurementStereotype() : Sequence(Property){
	var stereotypeAttr = MeasurementStereotype.allAttributes();
	var returnValue : Sequence(Property) = new Sequence(Property);
	
	for(attr in stereotypeAttr){
		if(not attr.name.startsWith("base_"))
		{ 
			returnValue.add(attr);
		}
	}
	return returnValue;	
}

/*
Returns a Sequence of description of stereotype MeasurementStereotype.
*/ 
operation Element getDescriptionOfMeasurementStereotypeAsSequence() : Sequence{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MeasurementStereotype"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


/*
Check if a Sequence of description of stereotype MeasurementStereotype is empty.
*/ 
operation Element isDescriptionOfMeasurementStereotypeEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("MeasurementStereotype"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
