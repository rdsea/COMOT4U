/*
Check if a Sequence of duration of stereotype PersistentManifestation is empty.
*/ 
operation Element isDurationOfPersistentManifestationEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("PersistentManifestation"), "duration");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
