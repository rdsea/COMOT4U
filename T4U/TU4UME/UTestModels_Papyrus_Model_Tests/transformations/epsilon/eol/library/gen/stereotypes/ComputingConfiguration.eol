}


/*
Check if a Sequence of description of stereotype ComputingConfiguration is empty.
*/ 
operation Element isDescriptionOfComputingConfigurationEmpty() : Boolean{
	var returnValues = new Sequence;
	var values = self.getValue(self.getStereotype("ComputingConfiguration"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
