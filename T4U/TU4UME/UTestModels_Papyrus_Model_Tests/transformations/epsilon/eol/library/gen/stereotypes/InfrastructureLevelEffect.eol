/*
Return the BaseElement from the applied Stereotype InfrastructureLevelEffect using UMLUtil native object
*/ 
operation InfrastructureLevelEffect getBaseElementOfInfrastructureLevelEffect() : Element{
	var baseElement = null;

	var plugin : new Native("at.ac.tuwien.dsg.epsilon.tools.SampleTool");
	var UMLUtil = plugin.getUMLUtil();
	baseElement =UMLUtil.getBaseElement(self);
	return baseElement;
}

/*
Return the JSON String from the applied Stereotype InfrastructureLevelEffect using UMLUtil native object
*/ 
